<div class="card-container">
    <div class="cards">
        <div class="layout">
            {{#each cars}}
            <card-element photo={{this.photo}} title="{{this.title}}" equip="{{this.equip}}" price="{{this.price}}"
                delivery="{{this.delivery}}" isOffer="{{this.isOffer}}" available="{{this.available}}"></card-element>
            {{/each}}
        </div>
    </div>
</div>

<script src="../public/components/card.js"></script>